{
    "name": "alexsoft/laravel-notifications-telegram",
    "description": "[DEPRECATED] Laravel 5.3 Notifications Telegram driver",
    "keywords": [
        "telegram-laravel-notifications",
        "laravel",
        "telegram",
        "notifications"
    ],
    "homepage": "https://github.com/alexsoft/laravel-notifications-telegram",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Plekhanov",
            "email": "alex@plekhanov.us",
            "homepage": "https://alex.plekhanov.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/notifications": "^5.3@dev",
        "illuminate/support": "^5.3@dev",
        "illuminate/events": "^5.3@dev"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Alexsoft\\LaravelNotificationsTelegram\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alexsoft\\LaravelNotificationsTelegram\\Test\\": "tests"
        }
    },
    "suggest": {
        "laravel-notification-channels/telegram": "This package is deprecated. Please use laravel-notification-channels vendor"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
